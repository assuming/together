{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "declaration": true,
    "lib": [
      "es2017",
    ],
    "outDir": "../scripts-build",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "."
  ]
}